<!--miniprogram/pages/MySelf/MySelf.wxml-->
<view class="container">
    <!-- 用户 openid -->
  <view class="userinfo">
    <button 
      open-type="getUserInfo" 
      bindgetuserinfo="onGetUserInfo"
      class="userinfo-avatar"
      style="background-image: url({{avatarUrl}})"
    ></button>
    <view>
      <open-data type="userNickName">用户名</open-data>
    </view>
  </view>

  <!-- 分割线 -->
  <view class="divLine"></view>

<navigator url="{{personurl}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__bd">{{text}}</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>

  <navigator url="{{mycluburl}}" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__bd">我的社团</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>

  <navigator url="../MyActivity/MyActivity" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__bd">我的活动</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>

    <navigator url="../CreateClub/CreateClub" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__bd">创建社团</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>

    <navigator url="../PM/PM" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__bd">管理（社长）</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>

    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__bd">开发人员</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>

    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__bd">分享小程序</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>

    <navigator url="" class="weui-cell weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__bd">联系我们</view>
    <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>
</view>
